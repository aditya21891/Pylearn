# this class is an example of abstraction 

classs Animal:
	def __init__(self,name):
	self.name=name

	def talk(self,sound):
		pass

animex=Animal("jenny")

print(animex.name)
print(animex.talk())


class Cat(Animal):

	def talk(self,sound):
		return "Meow"

pets=Cat("Ruby")
print(pets.name)
print(pets.talk)